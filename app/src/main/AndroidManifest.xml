<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rj.watersupply">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash"  android:theme="@style/Noaction">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewDetail"
            android:label="Show Detail"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".AddCustomer"
            android:label="Add Customer" />
        <activity
            android:name=".IncomeActivity"
            android:label="Income" />
        <activity
            android:name=".MyScheduleCalendarActivity"
            android:label="Calendar" />
        <activity
            android:name=".AddBottle"
            android:label="Add Bottle" />
        <activity
            android:name=".Login"
            android:theme="@style/Noaction">
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>